<p id="notice"><%= notice %></p>

<h1>About Us</h1>

<table>
  <thead>
    <tr>
      <th>First title</th>
      <th>Second title</th>
      <th>Third title</th>
      <th>First description</th>
      <th>Second description</th>
      <th>Third description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @about_us.each do |about_u| %>
      <tr>
        <td><%= about_u.first_title %></td>
        <td><%= about_u.second_title %></td>
        <td><%= about_u.third_title %></td>
        <td><%= about_u.first_description %></td>
        <td><%= about_u.second_description %></td>
        <td><%= about_u.third_description %></td>
        <td><%= link_to 'Show', about_u %></td>
        <td><%= link_to 'Edit', edit_about_u_path(about_u) %></td>
        <td><%= link_to 'Destroy', about_u, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New About U', new_about_u_path %>
